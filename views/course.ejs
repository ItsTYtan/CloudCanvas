<html lang-="en">
    <%- include('./partials/head.ejs') %>
    <body>
        <nav>
            <a class="nav-button" style="background-color:#ffb68e" href="/">
                <img src="/images/file-icon.png" alt="file-icon">
                <p class="label-small">Your Files</p>
            </a>
            <a class="nav-button">
                <img src="/images/settings-icon.png" alt="settings-icon">
                <p class="label-small">Settings</p>
            </a>
        </nav>
        <h4><%= course_name %></h4>
        <ul class="documents-list">
            <% for (let i = 0; i < folders.length; i++) { %>
                <li>
                    <div class="couse-file">
                        <a href=<%= 'folder/' + folders[i]._id %>>
                            <img src="/images/file-orange.png">
                            <p class="label-medium"><%= folders[i].folder_name %></p>
                        </a>
                    </div>
                </li>
            <% } %>
        </ul>
    </body>
</html>